{
  "presets": ["next/babel"],

  "plugins": [
    ["styled-components", {
      "ssr": true,
      "displayName": true,
      "preprocess": false
    }],

    ["module-resolver", {
      "root": ["."],
      "alias": {
        "components": "./src/components",
        "helpers": "./src/helpers",
        "@ui": "./src/ui"
      }
    }]
  ]
}
